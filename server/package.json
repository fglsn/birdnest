{
	"name": "birdnest-server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "NODE_ENV=production node build/index.js",
		"dev": "NODE_ENV=development ts-node-dev src/index.ts",
		"test": "NODE_ENV=test jest --verbose --runInBand --forceExit",
		"connect-db": "psql postgresql://localhost:5434/birdnest -U postgres",
		"connect-db:test": "psql postgresql://localhost:5435/birdnest-test -U postgres",
		"migration:create": "node node_modules/db-migrate/bin/db-migrate create -e local",
		"migrate": "db-migrate up -e local",
		"migrate:test": "db-migrate up -e test",
		"prettier": "prettier --config .prettierrc '**/*.ts' --write",
		"lint": "eslint --ext .ts .",
		"tsc": "tsc"
	},
	"author": "Ilona Shakurove",
	"license": "ISC",
	"dependencies": {
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.15",
		"@types/jest": "^29.2.5",
		"@types/pg": "^8.6.6",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"axios": "^1.2.2",
		"cors": "^2.8.5",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"dotenv": "^16.0.3",
		"eslint": "^8.31.0",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-async-handler": "^1.2.0",
		"fp-ts": "^2.13.1",
		"http": "^0.0.1-security",
		"io-ts": "^2.2.20",
		"jest": "^29.3.1",
		"pg": "^8.8.0",
		"postgres": "^3.3.3",
		"prettier": "^2.8.2",
		"psql": "^0.0.1",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.4",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4",
		"xml-js": "^1.6.11"
	}
}
